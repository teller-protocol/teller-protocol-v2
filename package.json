      "packages/services",
      "packages/subgraph"
    ]
  },
  "packageManager": "yarn@3.6.1",
  "devDependencies": {
    "eslint": "^7.32.0",
    "husky": "^8.0.1",
    "mustache": "^4.2.0",
    "prettier": "^2.4.1",
    "shx": "^0.3.3"
  }
}
