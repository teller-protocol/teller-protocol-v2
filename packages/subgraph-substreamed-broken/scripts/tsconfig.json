{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "lib": [
      "esnext"
    ],
    "types": [
      "node"
    ],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "moduleResolution": "node",
    "noEmit": true
  },
  "include": [
    "./**/*.ts"
  ]
}
