specVersion: 1.0.0
schema:
  file: ./src/schema.graphql
{{#if grafting.enabled}}
features:
  - grafting
{{/if}}
{{#if grafting.enabled}}
graft:
  base: {{grafting.base}}
  block: {{grafting.block}}
{{/if}}
dataSources:
  - kind: substreams
    name: Event  # ????
    network: {{network}}
    source:
      startBlock: {{block}}
      package:
        moduleName: map_events # Module name
        file: substreams/lendergrouppolygon-v0.1.0.spkg # Package
    mapping:
      kind: substreams/graph-entities
      apiVersion: 0.0.7
      file: ./src/mapping.ts
      handler: handleSubstreamGraphOutTrigger
  